<script>
    console.log('shortly  loaded', 'App Extension', 'version:1.42');
    var traffic_url = 'https://traffic.cut.live';
    var appProxyUrlExt = "/apps/shortly";
    var checkoutFlag = false;
    var cookie_duration = 1;

    var getParamsExt = function(url) {
        var paramsExt = {};
        var parserExt = document.createElement('a');
        parserExt.href = url;
        var queryExt = parserExt.search.substring(1);
        var letsExt = queryExt.split('&');
        for (let i = 0; i < letsExt.length; i++) {
            let pairExt = letsExt[i].split('=');
            paramsExt[pairExt[0]] = decodeURIComponent(pairExt[1]);
        }
        return paramsExt;
    };

    // format url parameters
    function formatParamsExt(paramss) {
        return "?" + Object
            .keys(paramss)
            .map(function(key) {
                return key + "=" + encodeURIComponent(paramss[key])
            })
            .join("&")
    }
    // start timer 
    function startTimerShortExt(duration, displayMints, displayScnd) {
        // console.log('startTimerShortExt')
        let timer = duration,
            minutes, seconds;
        setInterval(function() {
            minutes = parseInt(timer / 60, 10);
            seconds = parseInt(timer % 60, 10);

            minutes = minutes < 10 ? "0" + minutes : minutes;
            seconds = seconds < 10 ? "0" + seconds : seconds;

            displayMints.textContent = minutes;
            displayScnd.textContent = seconds;

            Cookies.set("discountly_timem", minutes, {
                expires: cookie_duration
            });
            Cookies.set("discountly_times", seconds, {
                expires: cookie_duration
            });

            if (--timer < 0) {
                if (document.getElementById('shortbar')) {
                    document.getElementById('shortbar').style.display = "none";
                }
                if (document.getElementById('discountly_show-b')) {
                    document.getElementById('discountly_show-b').style.display = "none";
                }
                sessionStorage.removeItem("discount_code");
                sessionStorage.removeItem("shortly_tid");
                Cookies.remove("discountly_timem");
                Cookies.remove("discountly_times");
                Cookies.remove("discount_code");
                Cookies.remove("shortly_tid");
            }
        }, 1000);
        if (document.getElementById('shortbar')) {
            document.getElementById('shortbar').style.display = 'block';
        }
    }
    // Fetch Request to call shopify proxy
    function proxyExt(params, callback) {
        var urlExt = appProxyUrlExt + formatParamsExt(params)
        fetch(urlExt, {
                method: 'POST'
            }).then(response => {
                return response.text()
            }).then(data => callback(data))
            .catch(err => {
                console.log(err);
            });
    }

    var refsExt;

    function startExt() {
        // get current url parameters
        refsExt = getParamsExt(window.location.href);
        // if parameters contains tid
        if (refsExt.tid) {
            if (sessionStorage.getItem('shortly_tid') !== refsExt.tid) {
                // clear old cookies and sessionStorage if tid is different 
                sessionStorage.removeItem("discount_code");
                sessionStorage.removeItem("shortly_tid");
                sessionStorage.setItem("shortly_tid", refsExt.tid);
                Cookies.remove("discountly_timem");
                Cookies.remove("discountly_times");
                Cookies.remove("discount_code");
                Cookies.remove("shortly_tid");
                Cookies.set('shortly_tid', `${refsExt.tid}`, {
                path: '/'
            });
            }
            var tidExt = refsExt.tid
                // get data of url using tid from server
            initExt(tidExt)
        } else {
            // if parameters does not have tid check in sessionStorage
            if ("shortly_tid" in sessionStorage) {
                var tidExt = sessionStorage.getItem('shortly_tid')
                if (tidExt !== refsExt.tid) {
                    // console.log('not same id')
                    if ("shortly_html" in sessionStorage) {
                        var shortdata = sessionStorage.getItem('shortly_html')
                        shortdata = JSON.parse(shortdata)
                        let bar = shortdata.bar
                        let discount = shortdata.discount
                        let discount_code = shortdata.discount_code
                        cookie_duration = shortdata.cookie_duration
                        var bodyExt = document.getElementsByTagName('body')[0];
                        var divExt = document.createElement('div');
                        divExt.innerHTML = bar;
                        bodyExt.insertBefore(divExt, bodyExt.firstChild)

                        Cookies.set('shortly_tid', `${tidExt}`, {
                            expires: cookie_duration,
                            path: '/'
                        });

                        sessionStorage.setItem("shortly_tid", tidExt);

                        if (document.querySelector("#shorttime") && document.querySelector('#shorttime').value > 0) {
                            timerExt()
                        } else {
                            if (document.querySelector('#shortTimer')) {
                                document.querySelector('#shortTimer').style.display = "none"
                            }
                            if (!refsExt.tid &&  document.querySelector('#shortbar')) {
                                document.querySelector('#shortbar').style.display = "none"
                            }
                            if (document.querySelector('#shorttimestatus') && document.querySelector('#shorttimestatus').value == 1 && document.querySelector('#shorttime').value == 0) {
                                document.querySelector('#shortbar').style.display = "block"
                            }
                        }
                        if (discount_code) {
                            applydiscountExt(discount_code)
                        }

                        if (document.getElementById('closeit_bottom')) {
                            document.getElementById('closeit_bottom').addEventListener('click', function() {
                                document.getElementById('discountly_show-b').style.display = 'none';
                            });
                        }
                        if (document.querySelector('#closeit_top')) {

                            document.getElementById('closeit_top').addEventListener('click', function() {
                                document.querySelector('#shortbar').style.display = 'none';
                            });
                        }

                    }
                } else {
                    initExt(tidExt)
                }
            }
        }
    }

    // get url data from short code
    function initExt(shortId) {

        let shortly_params = {
                query: 'bar',
                tid: shortId
            }
            //Proxy Call
        proxyExt(shortly_params, function(shortdata) {
            sessionStorage.setItem('shortly_html', shortdata)
            shortdata = JSON.parse(shortdata)
            console.log(shortdata);
            let bar = shortdata.bar
            let discount = shortdata.discount
            let discount_code = shortdata.discount_code
            cookie_duration = shortdata.cookie_duration;
            if (bar) {
                // document.body.innerHTML = bar+document.body.innerHTML
                var body = document.getElementsByTagName('body')[0];
                var div = document.createElement('div');
                div.innerHTML = bar;
                body.insertBefore(div, body.firstChild);
            }
            Cookies.set('shortly_tid', `${shortId}`, {
                expires: cookie_duration,
                path: '/'
            });
            sessionStorage.setItem("shortly_tid", shortId);
            sessionStorage.setItem('shortly_cookie_duration', cookie_duration);
            sessionStorage.setItem('shortly_shop_plan', shortdata.plan_id)
